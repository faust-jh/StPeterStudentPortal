<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F9D8FF"
    tools:context=".FacultyScheduleFragment">

    <TextView
        android:id="@+id/facschedule"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Schedule"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="@+id/facultyPortal5"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="@+id/facultyPortal5"
        app:layout_constraintTop_toBottomOf="@+id/facultyPortal5" />

    <ImageView
        android:id="@+id/userpic9"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginEnd="52dp"
        app:layout_constraintBottom_toBottomOf="@+id/facultyPortal5"
        app:layout_constraintEnd_toStartOf="@+id/facultyPortal5"
        app:layout_constraintTop_toTopOf="@+id/facultyPortal5"
        app:layout_constraintVertical_bias="0.615"
        app:srcCompat="@drawable/user" />

    <TextView
        android:id="@+id/facultyPortal5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Faculty Portal"
        android:textSize="25sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toStartOf="@+id/guideline3"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toTopOf="@+id/guideline4" />

    <ImageView
        android:id="@+id/notifbell9"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="52dp"
        app:layout_constraintBottom_toBottomOf="@+id/facultyPortal5"
        app:layout_constraintStart_toEndOf="@+id/facultyPortal5"
        app:layout_constraintTop_toTopOf="@+id/facultyPortal5"
        app:layout_constraintVertical_bias="0.615"
        app:srcCompat="@drawable/notifbell" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="41dp"
        android:layout_height="40dp"
        android:layout_marginTop="60dp"
        app:layout_constraintEnd_toEndOf="@+id/notifbell9"
        app:layout_constraintHorizontal_bias="0.578"
        app:layout_constraintStart_toStartOf="@+id/notifbell9"
        app:layout_constraintTop_toBottomOf="@+id/notifbell9"
        app:srcCompat="@drawable/ic_baseline_print_24" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="355dp"
        android:layout_height="290dp"
        android:layout_marginTop="56dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline5"
        app:layout_constraintEnd_toEndOf="@+id/facschedule"
        app:layout_constraintStart_toStartOf="@+id/facschedule"
        app:layout_constraintTop_toBottomOf="@+id/facschedule"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/schedule" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="1.0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>