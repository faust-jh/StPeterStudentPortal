<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F9D8FF"
    tools:context=".StudentInformation">

    <TextView
        android:id="@+id/textView58"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="Student Information"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="@+id/studentPortal3"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toStartOf="@+id/studentPortal3"
        app:layout_constraintTop_toBottomOf="@+id/studentPortal3" />

    <ImageView
        android:id="@+id/notifbell3"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="48dp"
        app:layout_constraintBottom_toBottomOf="@+id/studentPortal3"
        app:layout_constraintStart_toEndOf="@+id/studentPortal3"
        app:layout_constraintTop_toTopOf="@+id/studentPortal3"
        app:layout_constraintVertical_bias="0.461"
        app:srcCompat="@drawable/notifbell" />

    <ImageView
        android:id="@+id/userpic3"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginEnd="48dp"
        app:layout_constraintBottom_toBottomOf="@+id/studentPortal3"
        app:layout_constraintEnd_toStartOf="@+id/studentPortal3"
        app:layout_constraintTop_toTopOf="@+id/studentPortal3"
        app:layout_constraintVertical_bias="0.461"
        app:srcCompat="@drawable/user" />

    <TextView
        android:id="@+id/studentPortal3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="Student Portal"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.508"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/bottomNavigationView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintEnd_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent">

    </FrameLayout>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottomNavigationView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/bottom_nav_menu"/>

